{
  "compilerOptions": {
    "baseUrl": ".",
    "module": "ESNext",
    "target": "es2016",
    "lib": ["DOM", "ESNext"],
    "jsx": "preserve",
    "jsxFactory": "h",
    "jsxFragmentFactory": "Fragment",
    "strict": false,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "noFallthroughCasesInSwitch": false,
    "esModuleInterop": true,
    "incremental": false,
    "skipLibCheck": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "strictNullChecks": false,
    "noImplicitAny": false,
    "noImplicitThis": false,
    "noImplicitReturns": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": false,
    "checkJs": false,
    "types": [
      "vitest/importMeta",
      "vue",
      "@vue/runtime-core",
      "@vue/runtime-dom",
      "vite/client",
      "@types/node"
    ],
    "paths": {
      "@/*": ["src/*"]
    },
    "outDir": "dist",
    "declaration": true
  },
  "include": ["lib/**/*.ts", "lib/**/*.vue", "lib/**/*.tsx", "lib/types/*.d.ts"],
  "exclude": ["dist", "node_modules", "demo"],
}
